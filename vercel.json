{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "pages/api/chat.js": {
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/pages/api/chat"
    },
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}